{
	"dashboard": {
		"id": null,
		"title": "Hurricane ML Training - Log Based",
		"tags": [
			"ml",
			"airflow",
			"logs"
		],
		"timezone": "browser",
		"panels": [
			{
				"id": 1,
				"title": "Training Metrics from Logs",
				"type": "logs",
				"datasource": {
					"type": "loki",
					"uid": "grafanacloud-logs"
				},
				"targets": [
					{
						"expr": "{} |= \"TRAINING_METRIC:\"",
						"refId": "A"
					}
				],
				"options": {
					"showTime": true,
					"showLabels": false,
					"wrapLogMessage": false,
					"prettifyLogMessage": true,
					"enableLogDetails": true,
					"dedupStrategy": "none",
					"sortOrder": "Descending"
				},
				"gridPos": {
					"h": 12,
					"w": 24,
					"x": 0,
					"y": 0
				}
			},
			{
				"id": 2,
				"title": "Latest Training Run Summary",
				"type": "logs",
				"datasource": {
					"type": "loki",
					"uid": "grafanacloud-logs"
				},
				"targets": [
					{
						"expr": "{} |= \"=== ML TRAINING METRICS ===\"",
						"refId": "B"
					}
				],
				"gridPos": {
					"h": 8,
					"w": 24,
					"x": 0,
					"y": 12
				}
			}
		],
		"time": {
			"from": "now-2h",
			"to": "now"
		},
		"refresh": "30s"
	},
	"Transform": "AWS::Serverless-2016-10-31",
	"Resources": {
		"Api": {
			"Type": "AWS::Serverless::Api",
			"Properties": {
				"Name": {
					"Fn::Sub": [
						"${ResourceName} From Stack ${AWS::StackName}",
						{
							"ResourceName": "Api"
						}
					]
				},
				"StageName": "Prod",
				"DefinitionBody": {
					"openapi": "3.0",
					"info": {},
					"paths": {
						"/": {
							"get": {
								"responses": {}
							}
						}
					}
				},
				"EndpointConfiguration": "REGIONAL",
				"TracingEnabled": true
			}
		}
	}
}